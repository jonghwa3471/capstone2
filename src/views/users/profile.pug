extends ../base
include ../mixins/video

block content
  .profile__data
    if !user.avatarUrl
      span.header__icon 😃
    else
      img.avatar(src=`${user.avatarUrl}`)
    h3(style="font-weight: 700")= user.nickname
  .profile-wrapper
    .video-grid
      each video in user.videos
        +video(video)
      else
        span.empty__message 게시글이 존재하지 않습니다.
